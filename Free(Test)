-- Orion Library : https://github.com/shlexware/Orion/blob/main/Documentation.md

local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

OrionLib:MakeNotification({
	Name = "Stellar Hub",
	Content = "Thx For Using Stellar Hub",
	Image = "rbxassetid://4483345998",
	Time = 5
})

local Window = OrionLib:MakeWindow({Name = "Stellar Hub (Free)", HidePremium = false, IntroText = "Thx to Orion Library", Icon = "rbxassetid://8834748103", IntroIcon = "rbxassetid://8834748103", SaveConfig = false, ConfigFolder = "Config"})

local Tab = Window:MakeTab({
	Name = "Main",
