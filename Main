Tab:AddSlider({
	Name = "Walk Speed",
	Min = 0,
	Max = 100,
	Default = 16,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Walk Speed (normal 16)",
	Callback = function(ws)
		game.Players.LocalPlayer.character.Humanoid.WalkSpeed = ws
	end    
})

